// 1
concept_history
<- sc_node_class;
=> nrel_inclusion:
	concept_prehistory;
	concept_ancient;
	concept_antiquity;
	concept_medieval;
	concept_renaissance;
	concept_modern;
=> nrel_main_idtf:
	[История] (* <- lang_ru;; *);
=> nrel_main_idtf:
	[History] (* <- lang_en;; *);
=> nrel_note:
	[максимальный класс предметной области] (* <- lang_ru;; *);
=> nrel_note:
	[максимальный класс предметной области] (* <- lang_en;; *);
<- rrel_key_sc_element:
   	...
   	(*
    	=> nrel_main_idtf:
          	[Опр. (История)]
          	(* <- lang_ru;; *);;
      	<- definition;;
    	<= nrel_sc_text_translation:
           	...
           	(*
              	-> rrel_example:
               		[История — общий термин, описывающий как события прошлого, 
					так и память о прошлом, нахождение, сбор и интерпретацию 
					сведений об этих событиях. Период человеческого прошлого 
					до изобретения письменности и начала письменной истории 
					считается доисторическим периодом.]
               		(* <- lang_ru;; *);;
           	*);;
  	*);;


