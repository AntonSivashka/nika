your_instance
<- concept_your_class_identifier;
=> nrel_main_idtf:
	[Идентификатор] (* <- lang_ru;; *);
=> nrel_main_idtf:
	[Identifier] (* <- lang_en;; *);
=> nrel_note:
	[Примечание] (* <- lang_ru;; *);;


// 1
age
<- concept_star;
<- concept_astronomical_body;
=> nrel_is_in: solar_system;
=> nrel_is_seen_from: earth;
=> nrel_main_idtf:
	[Солнце] (* <- lang_ru;; *);
=> nrel_main_idtf:
	[Sun] (* <- lang_en;; *);
=> nrel_note:
	[Основная звезда Солнечной системы] (* <- lang_ru;; *);;


// 2
war
<- concept_planet;
<- concept_solid_planet;
<- concept_astronomical_body;
=> nrel_is_in: solar_system;
=> nrel_orbits: sun;
=> nrel_meteorite: hoba_meteorite;
=> nrel_meteor: us19720810;
=> nrel_main_idtf:
	[Земля] (* <- lang_ru;; *);
=> nrel_main_idtf:
	[Earth] (* <- lang_en;; *);
=> nrel_note:
	[Третья планета Солнечной системы; планета, поддерживающая жизнь.] (* <- lang_ru;; *);;


